{
    "type": "project",
    "license": "MIT",
    "name": "germdz/seven_colors-projects-tasks",
    "description": "Manage projects and their tasks taking into account: users, clients and organizations.",
    "keywords": [],
    "homepage": "https://github.com/gerMdz/SevenColors-projects-tasks",
    "authors": [
        {
            "name": "Gerardo Montivero",
            "email": "gerardo.montivero@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^3.3",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^2.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.13",
        "hashids/hashids": "^4.1",
        "knplabs/knp-time-bundle": "^1.11",
        "pagerfanta/doctrine-orm-adapter": "^3.3",
        "pagerfanta/twig": "^3.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpunit/phpunit": "^9.5",
        "sensio/framework-extra-bundle": "^6.0",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/messenger": "5.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/twig-bundle": "^5.4",
        "symfony/uid": "5.4.*",
        "symfony/ux-chartjs": "^1.3",
        "symfony/validator": "^5.4",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.4.*",
        "symfonycasts/reset-password-bundle": "^1.14",
        "symfonycasts/verify-email-bundle": "^1.11",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.4",
        "twig/markdown-extra": "^3.4",
        "twig/string-extra": "^3.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/var-dumper": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "zenstruck/foundry": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {},
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
